

@model System.Data.DataTable

<h1>Hello </h1>

<table class="table table-dark table-responsive">
    <thead>
        <tr>
            <th>Time Slot Id</th>
            <th>Present Time</th>
            <th>Time Slot Time</th>
            <th>Day ID</th>

        </tr>
    </thead>
    <tbody>

        @foreach (System.Data.DataRow item in Model.Rows)
        {
        <tr>
            <td>@item[0]</td>
            <td>@item[1]</td>
            <td>@item[2]</td>
            <td>@item[3]</td>
        </tr>
        }
    </tbody>
</table>